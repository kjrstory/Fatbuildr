[Distribution]
Distribution=debian
Release=sid

[Output]
Format=directory
Output=deb.img
OutputDirectory=/var/lib/fatbuildr/images/

[Content]
# Docs are useful to get examples from packages and manpages when digging
WithDocs=yes
Packages=cowbuilder,devscripts,neovim,man-db,less
ExtraTree=/usr/lib/fatbuildr/images/build/deb.extra/
