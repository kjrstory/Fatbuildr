{% extends "layout/base.html.j2" %}
{% block title %}{% if config['INSTANCE'] == 'all' %}{{ instance }} > {% endif %}{{ format }}{% endblock %}
{% block content %}
<h1>{% if config['INSTANCE'] == 'all' %}{{ instance }} > {% endif %}{{ format }} > {{ distribution }} > derivatives</h1>
<ul>
{% for derivative in derivatives %}
  <li><a href="{{ url_for('derivative', instance=instance, fmt=format, distribution=distribution, derivative=derivative, output='html') }}">{{ derivative }}</a></li>
{% endfor %}
</ul>
<a href="{{ url_for('distribution', instance=instance, fmt=format, distribution=distribution, output='json') }}">JSON</a>
{% endblock %}
