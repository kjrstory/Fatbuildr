{% extends "layout/base.html.j2" %}
{% block title %}{{ instance }} > {{ format }}{% endblock %}
{% block content %}
<h1>{{ instance }} > {{ format }} > {{ distribution }} > derivatives</h1>
<ul>
{% for derivative in derivatives %}
  <li><a href="{{ url_for('derivative_artefacts', instance=instance, fmt=format, distribution=distribution, derivative=derivative, output='html') }}">{{ derivative }}<a></li>
{% endfor %}
</ul>
<a href="{{ url_for('distribution_derivatives', instance=instance, fmt=format, distribution=distribution, output='json') }}">JSON</a>
{% endblock %}
