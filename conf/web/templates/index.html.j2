{% extends "layout/base.html.j2" %}
{% block title %}instances{% endblock %}
{% block content %}
<h1>Available instances:</h1>
<ul>
{% for instance in instances %}
  <li><a href="{{ url_for('registry', instance=instance.id, output='html') }}">{{ instance.id }} ({{ instance.name }})</a></li>
{% endfor %}
</ul>
<a href="{{ url_for('index', output='json') }}">JSON</a>
{% endblock %}
