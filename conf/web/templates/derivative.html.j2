{% extends "layout/base.html.j2" %}
{% block title %}{{ instance }} > {{ format }} > {{ distribution }} > {{ derivative }}{% endblock %}
{% block content %}
<h1>{{ instance }} > {{ format }} > {{ distribution }} > {{ derivative }} > artefacts </h1>
<ul>
{% for artefact in artefacts %}
  <li>
    <a href="/{{ instance }}/registry/{{ format }}/{{ distribution }}/{{ derivative}}/{{ artefact.architecture }}/{{ artefact.name }}">
      {{ artefact.name }} {{ artefact.architecture }} {{ artefact.version }}
    </a>
  </li>
{% endfor %}
</ul>
<a href="../{{ derivative }}.json">JSON</a>
{% endblock %}
